{
  "name": "@haorama/nestjection",
  "version": "0.5.5",
  "description": "Nestjs integration for Objection+Knex",
  "scripts": {
    "prepublish": "tsc",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "prepublish:npm": "npm run build",
    "dev": "nest start --path ./tests/tsconfig.json --config ./tests/nest-cli.json --watch",
    "publish:next": "npm publish --access public --tag next",
    "lint": "eslint \"lib/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haorama/nestjection.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nestjs",
    "objection",
    "js",
    "knex",
    "nestjs-module",
    "orm"
  ],
  "author": "Michael Lazuardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haorama/nestjection/issues"
  },
  "homepage": "https://github.com/haorama/nestjection#readme",
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/core": "^7.0.0 || ^8.0.0",
    "knex": "^0.95",
    "objection": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-fastify": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.0",
    "knex": "^0.95.10",
    "mysql2": "^2.2.5",
    "objection": "^2.0.0",
    "prettier": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "glob": "^7.1.7",
    "lodash.merge": "^4.6.2"
  }
}
