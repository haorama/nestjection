{
  "name": "@mlazuardy/nestjection",
  "version": "0.1.3",
  "description": "Nestjs integration for Objection+Knex",
  "scripts": {
    "prepublish": "tsc",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.json",
    "publish:npm": "npm publish --access public",
    "prepublish:npm": "npm run build",
    "dev": "nest start --path ./tests/tsconfig.json --config ./tests/nest-cli.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlazuardy/nestjection.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "nestjs",
    "objection",
    "js",
    "knex",
    "nestjs-module"
  ],
  "author": "Michael Lazuardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlazuardy/nestjection/issues"
  },
  "homepage": "https://github.com/mlazuardy/nestjection#readme",
  "peerDependencies": {
    "@nestjs/common": "^7.0.0|^8.0.0",
    "@nestjs/core": "^7.0.0|^8.0.0",
    "knex": "^0.21.20",
    "objection": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/platform-fastify": "^8.0.6",
    "@types/node": "^15.3.1",
    "dotenv": "^10.0.0",
    "knex": "^0.21.20",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.7",
    "objection": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.0.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
